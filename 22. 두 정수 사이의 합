class Solution {
    fun solution(a: Int, b: Int): Long {
        var num: Long = 0
        if(a<b){
            for (i in a .. b){
                num += i
            }
            return num
        }
        else if(a>b){
            for (i in b .. a){
                num += i
            }
            return num
        }
        else
            return a.toLong()
    }
}
