class Solution {
    fun solution(angle: Int): Int {
        
        if (angle in 1 .. 180){
            var answer = when(angle){
                in 1 .. 89 -> 1
                90 -> 2
                in 91 .. 179 -> 3
                180 -> 4
                else -> 0
            }
            return answer
        }
        else return 0
    }
}
